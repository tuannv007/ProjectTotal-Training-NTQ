<resources>
    <style name="CustomTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="contentInsetStart">0dp</item>
        <item name="contentInsetEnd">0dp</item>
    </style>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="textViewStyleSetting">
       <item name="android:textStyle">bold</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/textHm</item>
    </style>
    <style name="viewLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@android:color/black</item>
    </style>
    <style name="styleImageSetting">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:layout_weight">0.4</item>
        <item name="android:src">@drawable/edit_my_profile_arrow_right</item>
    </style>
    <style name="editTextChangePassword">
        <item name="android:inputType">textPassword</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:background">@drawable/background_button_main</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="textChangePassword">
        <item name="android:layout_margin">6dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="textViewLogin">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="EditTextLogin">
       <item name="android:singleLine">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:gravity">end|center</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:ellipsize">start</item>
        <item name="android:textSize">@dimen/textSizeEmail</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="textViewRecenReport">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_weight">3.5</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="imageViewRecenreport">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">0.5</item>
    </style>
    <style name="lineDashRecenReport">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2.5dp</item>
        <item name="android:background">@drawable/line_divider</item>
        <item name="android:layerType">software</item>
    </style>
    <style name="textViewPoint">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="imageViewPoint">
        <item name="android:layout_marginTop">@dimen/textHm</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
   <style name="textSearchEthnicity">
        <item name="android:layout_marginLeft">10dp</item>
       <item name="android:layout_marginStart">10dp</item>
       <item name="android:gravity">center_vertical</item>
       <item name="android:layout_width">0dp</item>
       <item name="android:layout_height">match_parent</item>
       <item name="android:layout_weight">1</item>
       <item name="android:textColor">@android:color/black</item>
       <item name="android:textSize">@dimen/textHm</item>
   </style>
    <style name="checkBoxSearchEthnicity">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">0.15</item>
        <item name="android:layout_height">match_parent</item>
    </style>
   <style name="buttonSearchSetting">
       <item name="android:layout_margin">5dp</item>
       <item name="android:background">@drawable/background_login_button</item>
       <item name="android:layout_width">0dp</item>
       <item name="android:layout_weight">1</item>
       <item name="android:layout_height">@dimen/textButton</item>
   </style>
    <style name="textViewBigEditProfile">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginStart">10dp</item>
    </style>
    <style name="textViewSmallEditProfile">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginStart">10dp</item>
    </style>
    <style name="imageViewEditProfile">
        <item name="android:layout_width">10dp</item>
        <item name="android:src">@drawable/edit_my_profile_arrow_right</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_margin">10dp</item>
    </style>
    <style name="relativeLayoutEditProfile">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1.1</item>
    </style>
    <style name="textViewDialogEditProfile">
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">4</item>
        <item name="android:layout_height">match_parent</item>
    </style>
   <style name="radioButtonEditProfile">
       <item name="android:layout_width">0dp</item>
       <item name="android:layout_height">match_parent</item>
       <item name="android:layout_weight">0.5</item>
   </style>
    <style name="ButtonWellcome">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_margin">7dp</item>
        <item name="android:background">@drawable/border_button_wellcome</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="MenuSearchFriend">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>
</resources>
